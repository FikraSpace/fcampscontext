rce":"app.js","original":{"line":127,"column":9}},{"generated":{"line":187,"column":24},"source":"app.js","original":{"line":127,"column":0}},{"generated":{"line":187,"column":25},"source":"app.js","original":{"line":127,"column":16}},{"generated":{"line":187,"column":54},"source":"app.js","original":{"line":127,"column":17}},{"generated":{"line":187,"column":57},"source":"app.js","original":{"line":127,"column":16}},{"generated":{"line":187,"column":64},"source":"app.js","original":{"line":127,"column":0}},{"name":"document","generated":{"line":187,"column":66},"source":"app.js","original":{"line":127,"column":25}},{"generated":{"line":187,"column":74},"source":"app.js","original":{"line":127,"column":33}},{"name":"getElementById","generated":{"line":187,"column":75},"source":"app.js","original":{"line":127,"column":34}},{"generated":{"line":187,"column":89},"source":"app.js","original":{"line":127,"column":25}},{"generated":{"line":187,"column":90},"source":"app.js","original":{"line":127,"column":49}},{"generated":{"line":187,"column":96},"source":"app.js","original":{"line":127,"column":25}},{"generated":{"line":187,"column":97},"source":"app.js","original":{"line":127,"column":0}}],"sources":{"app.js":"import React from 'react'\nimport ReactDOM from 'react-dom'\nimport styled from 'styled-components'\n\nlet Button = styled.button`\n  background-color: #466AB3;\n  padding: 10px;\n  border-radius: 8px;\n  border: none;\n  color: white;\n  font-weight: bold;\n  min-width: 100px;\n`\n\nlet Context = React.createContext()\n\n\nlet Navigation = styled.header`\n  background-color: #fff;\n  height: 120px;\n  display: flex;\n  justify-content: space-between;\n  align-items: center;\n  padding: 0px 10%;\n`\n\n\nclass Header extends React.Component {\n\n  constructor() {\n    super()\n    this.state = {\n      jobs: [{}, {}, {}]\n    }\n  }\n\n  render() {\n    return (\n      <Context.Consumer>\n        {\n          (ctx) => {\n            return (\n              <Navigation>\n                <img width=\"120px;\" src={require('./assets/logo.png')} />\n                <Button onClick={() => {\n                  console.log(ctx.actions.addJob())\n                }}>Post A Job</Button>\n              </Navigation>\n            )\n          }\n        }\n      </Context.Consumer>\n    )\n  }\n}\n\n\nlet Container = styled.main`\n  background-color: red;\n  min-height: 500px;\n  padding: 10px 10%;\n  \n`\nlet Job = styled.div`\n  height: 80px;\n  border: 1px solid;\n  background: #fff;\n  border-radius: 4px;\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  font-family: sans-serif;\n  font-size: 2rem;\n  margin-top: 20px;\n`\n\nclass JobsList extends React.Component {\n  constructor() {\n    super()\n  }\n  render() {\n    return (\n      <Context.Consumer>\n        {(ctx) => {\n          return <Container>\n            {\n              ctx.state.jobs.map((item, i) => {\n                return <Job key={i}>Job #{i}</Job>\n              })\n            }\n          </Container>\n        }}\n      </Context.Consumer>\n    )\n  }\n}\n\n\nclass App extends React.Component {\n\n  constructor() {\n    super()\n    this.state = {\n      jobs: [{}]\n    }\n  }\n\n  render() {\n    return (\n      <Context.Provider value={{\n        state: this.state, \n        actions: {\n          addJob:  ()=>{\n            let jobs = this.state.jobs\n            jobs.push({})\n            this.setState({ jobs: jobs })\n          }\n        }\n      }}>\n        <Header />\n        <JobsList />\n      </Context.Provider>\n    )\n  }\n}\n\nReactDOM.render(<App />, document.getElementById('root'))"},"lineCount":null}},"hash":"0e0b650334139bc02f2293c9dc2c696b","cacheData":{"env":{}}}